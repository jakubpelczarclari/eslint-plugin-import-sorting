{
  "name": "eslint-plugin-isort",
  "description": "",
  "license": "ISC",
  "author": "Jeff <rustyangel@gmail.com> (https://tidaltheory.co)",
  "homepage": "https://github.com/stormwarning/eslint-plugin-isort",
  "repository": "stormwarning/eslint-plugin-isort",
  "version": "1.0.0",
  "scripts": {
    "precommit": "lint-staged && npm test",
    "lint": "prettier-eslint '**/*.js' --list-different",
    "semantic-release": "semantic-release",
    "test": "npm run lint"
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "devDependencies": {
    "@zazen/eslint-config": "^1.0.0",
    "@zazen/semantic-release": "^0.1.1",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "remark-cli": "^6.0.1",
    "remark-github": "^7.0.7",
    "remark-preset-lint-markdown-style-guide": "^2.1.3",
    "remark-toc": "^5.1.1",
    "semantic-release": "^15.13.17"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "import",
    "order",
    "sort"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "release": {
    "extends": "@zazen/semantic-release"
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-markdown-style-guide",
      [
        "lint-maximum-line-length",
        false
      ],
      [
        "lint-list-item-spacing",
        false
      ],
      "github",
      "toc"
    ],
    "settings": {
      "commonmark": true,
      "bullet": "-",
      "listItemIndent": 1,
      "rule": "-",
      "ruleSpaces": false,
      "strong": "*",
      "emphasis": "*"
    }
  }
}
